{
    "name": "galandas/togglebbcode",
    "type": "phpbb-extension",
    "description": "This extension allows us to insert in topics one Spoiler Toggle Open/Close on our Board with a very nice JQuery effect.",
    "homepage": "http://phpbb3world.altervista.org",
    "version": "1.1.1",
    "time": "2016-11-19",
    "keywords": [
        "phpbb",
        "extension",
        "togglebbcode"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Galandas",
            "homepage": "http://phpbb3world.altervista.org",
            "email": "manzarek@live.it",
            "role": "Amateur Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": ">=3.1.10,<3.2.0"
    },
    "extra": {
        "display-name": "Toggle BBcode",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.10,<3.2.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/toggle_bbcode",
            "filename": "version_check"
        }
    }
}