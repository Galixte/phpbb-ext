{
    "name": "galandas/tabsmenu",
    "type": "phpbb-extension",
    "description": "This Extension allows you to organize content in a interface has tabs, with the desired content that appear when a tab is clicked, simple but very versatile.",
    "homepage": "http://phpbb3world.altervista.org",
    "version": "1.0.0",
    "time": "2017-05-23",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Galandas",
            "email": "manzarek@live.it",
            "homepage": "http://phpbb3world.altervista.org",
            "role": "Amateur Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "phpbb/phpbb": ">=3.1.10,<3.2.0"
    },
    "extra": {
        "display-name": "Tabs Menu in index",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.10,<3.2.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/tabs_menu_in_index",
            "filename": "version_check"
        }
    }
}