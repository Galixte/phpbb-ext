{
    "name": "kinerity/fulllinks",
    "type": "phpbb-extension",
    "description": "An extension for phpBB that displays full links within posts.",
    "homepage": "https://www.project-w.org",
    "version": "1.0.0",
    "time": "2018-02-05",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "kinerity",
            "email": "kinerity@yahoo.com",
            "homepage": "https://www.layer-3.org",
            "role": "Extension Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "phpbb/phpbb": ">=3.2.0,<3.3.0@dev",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Full Links",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0,<3.3.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/full_links",
            "filename": "version_check"
        }
    }
}