{
    "name": "threedi/pfbbcode",
    "type": "phpbb-extension",
    "description": "Enables BBCodes to Text and String type Profile Fields.",
    "version": "1.0.1",
    "time": "2018-03-08",
    "keywords": [
        "phpbb",
        "extension",
        "profile",
        "field",
        "text",
        "string",
        "BBCode"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "3Di",
            "email": "three3di@hotmail.it",
            "homepage": "http://3di.space",
            "role": "Developer"
        },
        {
            "name": "javiexin",
            "email": "javiexin@gmail.com",
            "homepage": "",
            "role": "Past Contributor and Lead Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.2.1,<3.3.0@dev"
    },
    "extra": {
        "display-name": "BBCode Enabled Profile Fields",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.1,<3.3.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/bbcode_enabled_profile_fields",
            "filename": "version_check"
        }
    }
}