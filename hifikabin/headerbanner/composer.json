{
    "name": "hifikabin/headerbanner",
    "type": "phpbb-extension",
    "description": "Replaces everything in the Header with a banner and gives the option to move the Search to the NavBar. This extension is for phpBB3.2.x ONLY",
    "homepage": "http://phpbb.hifikabin.me.uk",
    "version": "3.2.1",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "James Newcombe",
            "username": "HiFiKabin",
            "email": "webmaster@hifikabin.me.uk",
            "homepage": "http://phpbb.hifikabin.me.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "phpbb/phpbb": ">=3.2.0@dev",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Header Banner",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/header_banner",
            "filename": "version_check"
        }
    }
}